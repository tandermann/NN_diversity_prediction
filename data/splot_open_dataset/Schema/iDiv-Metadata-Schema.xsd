    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="temporalCover">
    <xs:sequence>
      <xs:element name="Date_of_first_Record"
                  type="date"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>The date of the first record of the dataset. Type value for the field Year and pick values from the dropdownlist for the field Month and Day.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Date_of_last_Record"
                  type="date"
                  minOccurs="0">
        <xs:annotation>
          <xs:documentation>The date of the last record of the dataset. Type value for the field Year and pick values from the dropdownlist for the field Month and Day.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Temporal_Resolution"
                  type="temporal"
                  minOccurs="0">
      </xs:element>
    </xs:sequence>
  </xs:complexType>

